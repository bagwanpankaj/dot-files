	ci = commit
	br = branch
	co = checkout
	df = diff
	lg = log -p
	lol = log --graph --decorate --pretty=oneline --abbrev-commit
	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
	ls = ls-files
  c = commit -m
  a = add
  dic = diff --cached
  pl = pull
  ps = push
  plre = pull --rebase
  bra = branch -a
